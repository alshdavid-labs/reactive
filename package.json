{
  "name": "@alshdavid/reactive",
  "version": "1.0.0",
  "description": "Generic object mutation observation",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "David Alsh",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn compile && yarn optimize",
